    "redux-devtools-extension": "^2.13.7",
    "rimraf": "^2.6.2",
    "style-loader": "^1.0.0",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^1.4.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@hot-loader/react-dom": "^16.8.6",
    "classnames": "^2.2.6",
    "memoize-one": "^5.0.5",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
