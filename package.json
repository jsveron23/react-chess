{
  "name": "react-chess",
  "version": "0.6.0",
  "description": "Chess game",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "jest --config ./tests/jest.config.js --detectOpenHandles",
    "coverage": "npm test -- --coverage --coverageReporters=text-lcov",
    "coveralls": "npm run coverage | coveralls",
    "lint": "eslint src",
    "clean": "rimraf public",
    "prebuild": "npm run clean",
    "bundle:dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.js",
    "bundle:prod": "webpack --config ./scripts/webpack/webpack.config.js --env.production --display-error-details",
    "build": "npm run bundle:prod",
    "start": "npm run bundle:dev",
    "deploy": "./scripts/gh-pages/deploy.sh",
    "update": "npm-check -u",
    "sync:master": "git reset --hard origin/master"
  },
  "keywords": [
    "react",
    "chess"
  ],
  "author": "Tony Jin <jsveron23@gmail.com>",
  "license": "MIT",
  "devDependencies": {
